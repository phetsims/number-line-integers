{
  "name": "number-line-integers",
  "version": "1.3.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/number-line-integers.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "phetLibs": [
      "number-line-common",
      "twixt"
    ],
    "requirejsNamespace": "NUMBER_LINE_INTEGERS",
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simulation": true,
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "NUMBER_LINE_INTEGERS/screen.explore",
      "NUMBER_LINE_INTEGERS/screen.generic"
    ],
    "simFeatures": {
      "supportedRegionsAndCultures": [
        "multi",
        "africa",
        "africaModest",
        "asia",
        "latinAmerica",
        "oceania",
        "usa"
      ],
      "defaultRegionAndCulture": "multi",
      "supportsDynamicLocale": true
    }
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}